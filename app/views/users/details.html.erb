<%= render 'layouts/header' %>
<div>
  <ul class="i2">
    <% @restaurantdetails.each do |key, value| %>
    <li>
      <h2><%=key %></h2>
      <ul>
        <div class="row">
        <%value.each do |item| %>
          <div class="col-md-4">
            <div class="i3">
              <div>
                <span><%= item["name"] %></span>
              </div>
              <div>
                <span>$ <%= item[:cost] %></span>
              </div>
            </div>
          </div>
        <%end %>
        </div>
      </ul>
    </li>
    <%end %>
  </ul>
</div>
<%= render 'layouts/footer' %>